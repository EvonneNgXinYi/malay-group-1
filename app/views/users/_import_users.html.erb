<h1><%= t ".heading" %></h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
  	<%= form_tag import_users_path, multipart: true, id: "import_users_form" do %>
  	  <%= label_tag :attachment %>
      <%= file_field_tag :attachment %>
      <hr>
      <%= submit_tag t(".import"), class: "btn btn-primary" %>
      <%= link_to t(".return"), users_path %>
    <% end %>
  </div>
</div>
